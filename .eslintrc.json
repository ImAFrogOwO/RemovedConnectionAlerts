{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["warn", { "code": 120, "ignoreComments": true }],
        "no-unused-vars": ["warn"]
    }
}
